<div class="container">
    <div class="vlt-page vlt-page-plain">
        <div class="vlt-dialog col-md-10 col-md-offset-1 top-50 vlt-tree-move">
            <div class="vlt-dialog-content">
                <div class="vlt-dialog-header">
                    <h2>Move {{typeName}}</h2>
                </div>
                <div class="vlt-dialog-body">

                    <div class="col-md-9 col-md-offset-3">

                        <div class="form-group">
                            <div class="vlt-tree">
                                {{view Vaultier.Document.TreeContainer controllerBinding="treeController"}}
                            </div>
                            <div class="clearfix"></div>
                        </div>

                        <div class="clearfix"></div>

                    </div>

                    <div class="col-md-10 col-md-offset-1">
                        <hr class="top-0"/>
                    </div>

                    <div class="col-md-9 col-md-offset-3">
                        <div class="help-block">
                            All granted permissions will be moved as well.
                        </div>
                    </div>

                    <div class="clearfix"></div>

                </div>
                <div class="vlt-dialog-footer">
                    <a href="javascript:window.history.go(-1)" class="btn btn-default btn-sm">
                        <span class="glyphicon glyphicon-chevron-left"></span>
                        Back
                    </a>
                    <button {{ action move}} {{bind-attr disabled=moveDisabled }} class="btn btn-primary">
                        <span class="glyphicon glyphicon-ok"></span>
                        Move
                    </button>
                </div>

            </div>
        </div>
        <div class="clearfix"></div>
    </div>
</div>

