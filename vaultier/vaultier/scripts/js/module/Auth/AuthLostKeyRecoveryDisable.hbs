<div class="container">
    <div class="vlt-page vlt-page-plain">
        <div class="vlt-dialog col-md-8 col-md-offset-2 col-xs-12 top-50">
            <div class="vlt-dialog-content">
                <form class="form-horizontal" role="form">

                    <div class="vlt-dialog-header">
                        <h2>Disable Your Workspaces</h2>
                    </div>
                    <div class="vlt-dialog-body">
                        <div class="col-md-8 col-md-offset-2">
                            <div class="vlt-dialog-body">
                                <div class="text-center">
                                    Your Key Was Disabled.
                                </div>
                            </div>

                        </div>
                        <div class="clearfix"></div>
                    </div>
                    <div class="vlt-dialog-footer">

                        <div class="pull-right vlt-right-buttons">
                            {{#link-to 'AuthRegister' class="btn btn-primary"}}
                                Register
                            {{/link-to}}
                        </div>

                        <div class="clearfix"></div>

                    </div>
                </form>
            </div>
        </div>
        <div class="clearfix"></div>
    </div>
</div>