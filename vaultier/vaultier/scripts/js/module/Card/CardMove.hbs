<div class="container">
    <div class="vlt-page vlt-page-plain">
        <div class="vlt-dialog col-md-8 col-md-offset-2 top-50">
            <div class="vlt-dialog-content">
                <div class="vlt-dialog-header">
                    <h2>Move Card to Another Vault</h2>
                </div>
                <div class="vlt-dialog-body">
                    <h4>Please select target vault</h4>

                    <div class="vlt-tree">
                        {{view view.Tree content=treeNodes}}
                    </div>
                    <div class="help-block">
                        All granted permissions will be moved as well
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="vlt-dialog-footer">
                    <a href="javascript:window.history.go(-1)" class="btn btn-default btn-sm">
                        <span class="glyphicon glyphicon-chevron-left"></span>
                        Back
                    </a>
                    <button {{ action save}} {{bind-attr disabled=moveDisabled }} class="btn btn-primary">
                        <span class="glyphicon glyphicon-ok"></span>
                        Move
                    </button>
                </div>

            </div>
        </div>
        <div class="clearfix"></div>
    </div>
</div>

